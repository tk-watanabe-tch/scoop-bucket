    "hash": "d2e161077084c1c2bf0e42836c0df5975ef424b0e5cebd83db301361fd26f05a",
    "installer": {
        "script": [
            "if ((Get-Host).Version.Majar -gt 5 -and (Get-Module | Where-Object Name -eq \"APPX\").Length -eq 0) {",
            "  Import-Module APPX -usewindowspowershell",
            "}",
            "Add-AppxPackage -Path \"$dir\\winget.appxbundle\""
        ]
    },
    "uninstaller": {
        "script": [
            "if ((Get-Host).Version.Majar -gt 5 -and (Get-Module | Where-Object Name -eq \"APPX\").Length -eq 0) {",
            "  Import-Module APPX -usewindowspowershell",
            "}",
            "Get-AppxPackage -Name Microsoft.Winget.Source | Remove-AppxPackage",
            "Get-AppxPackage -Name Microsoft.DesktopAppInstaller | Remove-AppxPackage"
        ]
    },
    "checkver": {
        "url": "https://github.com/microsoft/winget-cli/releases",
        "regex": "tag/v-([\\w.-]+)\""
    },
    "autoupdate": {
        "url": "https://github.com/microsoft/winget-cli/releases/download/v-$version/Microsoft.DesktopAppInstaller_8wekyb3d8bbwe.appxbundle#/winget.appxbundle"
    }
